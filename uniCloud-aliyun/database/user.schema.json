{
	"bsonType": "object",
	"required": ["password","phone"],
	"permission": {
		"read": false,
		"create": false,
		"update": false,
		"delete": false
	},
	"properties": {
		"_id": {
			"description": "ID，系统自动生成"
		},
		"password": {
			"bsonType": "string",
			"title": "密码",
			"description": "用户密码"
		},
		"phone":{
			"bsonType": "string",
			"title": "电话",
			"description": "用户电话号码"
		},
		"register_time":{
			"bsonType": "timestamp",
			"title": "注册时间",
			"description": "用户注册时间",
			"forceDefaultValue":{
				"$env": "now"
			}
		},
		"email":{
			"bsonType": "string",
			"title": "邮箱",
			"description": "用户邮箱"
		}
	}
}